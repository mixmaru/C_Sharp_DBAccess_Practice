<Window x:Class="フォーカス移動練習.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:フォーカス移動練習"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <TextBox />
            <TextBox />
            <ComboBox ItemsSource="{Binding}" />
            <ComboBox ItemsSource="{Binding}" IsEnabled="False" />
            <TextBox />
            <RadioButton  Grid.Row="0" x:Name="rbA1" GroupName="Group1">Group1A</RadioButton>
            <RadioButton  Grid.Row="1" x:Name="rbB1" GroupName="Group1">Group1B</RadioButton>
            <RadioButton  Grid.Row="2" x:Name="rbC1" GroupName="Group1">Group1C</RadioButton>
        </StackPanel>
    </Grid>
</Window>
